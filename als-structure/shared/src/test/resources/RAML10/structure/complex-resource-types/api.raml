#%RAML 1.0
title: Example API
version: v1

traits:
  secured:
    description: Some requests require authentication
    queryParameters:
      get: # e.g. get:
        description: A token pair is required  # e.g. A get-token pair...
        example: h8duh3uhhu38   # e.g. get=h8duh3uhhu38
  otherT:
    description: otherTrait
resourceTypes:
  rt:
    put:
      responses:
        300:
/endpoint:
  type: rt
  is:
    - secured
  get:
    is:
      - otherT
    responses:
      200: