#%RAML 1.0
title: Api

types:
  SimpleType:
    type: object
    properties:
      value1: string
      value2:
        type: string
        xml:
          attribute: true
          name: attribute
  ComplexType:
    type: object
    properties:
      prop1: string
      prop2: SimpleType
    example:
      <ComplexType>

        <prop2 attribute='asd'>
          <value1>asd</value1>
        </prop2>
      </ComplexType>
