#%RAML 1.0 Library
types:
  expression:
    type: string
    (validatorProvider): "script to validate expressions in ide"
    (completionProvider): "script to complete in expressions in ide"
    (location):
        x: 5

  script:
    type: string
annotationTypes:
  requiredWhen:
    allowedTargets: TypeDeclaration
    properties:
      value: expression
  validWhen:
    allowedTargets: TypeDeclaration
    properties:
      value: expression
  condition:
    allowedTargets: Trait
    properties:
      value: expression
  depricated:
  validatorProvider: script
  completionProvider: script
  location:
    properties:
      x: number
      y: number
