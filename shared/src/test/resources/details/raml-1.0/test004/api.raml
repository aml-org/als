#%RAML 1.0
title: API
securitySchemes:
  basic:
    type: Basic Authentication
traits:
  main:
    description: main trait
  ba*se:
    is:  [main]
    securedBy:
      - basic
      - null
    protocols:
      - HTTPS
    displayName: base
    usage: method
    queryParameters:
      paging: number
baseUri: /some/base/uri

/someChildUri:
  get:
    responses:
      200:
        body:
          application/xml:
            description: some very useful resource
  /anotherChild:
    put:
      body:
        application/json:
          description: another very useful resource
