#%RAML 1.0 DataType
type: object
properties: 
  transactionID: string
  fromAccount: !include Account.raml
  toAccount: !include Account.raml
  transactionType: 
    enum: 
      - atm 
      - deposit 
      - cashWithdrawal
      - sepa
      - online
  transactionName?: string
  transactionAmount: !include Money.raml
  newAccountBalance: !include Money.raml
  postedAt: datetime
  completedAt?: datetime
example: !include ../examples/TransactionExample.raml