#%RAML 1.0 DataType
type: object
properties: 
  errorCode:
    type: string
    pattern: ^[A-Z]{3}$
    enum:
      - ERR
      - WRN
      - FTL
  errorMsg:
    type: string
    maxLength: 80
    required: false
    example: This is an error
examples:
  error:
    errorCode: ERR
    errorMsg: This is an error
  warning:
    errorCode: WRN