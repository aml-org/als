#%RAML 1.0 Trait
description: "A description"
headers:
  client_id:
    description: The Client-ID of the Application.
    type: string
    pattern: ^[0-9a-fA-F]{32,32}
    example: '1234567890abcdefABCDEF1234567890'
  client_secret:
    description: The Client-Secret of Application.
    type: string
    pattern: ^[0-9a-fA-F]{32,32}
    example: '1234567890ABCDEFabcdef1234567890'