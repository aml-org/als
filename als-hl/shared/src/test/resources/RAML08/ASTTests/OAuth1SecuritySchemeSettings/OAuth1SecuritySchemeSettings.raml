#%RAML 0.8
title: API
securitySchemes:
  - oauth:
      type: OAuth 1.0
      settings:
        requestTokenUri: uri
        authorizationUri: uri
        tokenCredentialsUri: uri
        signatures: [HMAC-SHA1, RSA-SHA1, PLAINTEXT]
      describedBy:
        queryParameters:
          abc:
            type: number
        headers:
          If-Match:
        responses:
          200:
          201:
