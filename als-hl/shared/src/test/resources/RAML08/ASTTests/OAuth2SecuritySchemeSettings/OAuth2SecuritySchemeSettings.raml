#%RAML 0.8
title: API
securitySchemes:
  - oauth:
      type: OAuth 2.0
      settings:
        accessTokenUri: uri
        authorizationGrants: [authorization_code, password]
        authorizationUri: uri
        scopes: [ ADMINISTRATOR ]
      describedBy:
        queryParameters:
          abc:
            type: number
        headers:
          If-Match:
        responses:
          200:
          201:
