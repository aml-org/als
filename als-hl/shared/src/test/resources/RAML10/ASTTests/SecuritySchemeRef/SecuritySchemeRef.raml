#%RAML 1.0
title: API
securitySchemes:
  oauth:
    type: OAuth 2.0
    settings:
      accessTokenUri: uri
      authorizationGrants: authorization_code
      authorizationUri: uri
    describedBy:
      queryParameters:
        abc: number
      headers:
        If-Match:
      responses:
        200:
        201:
/res:
  securedBy: [oauth: { accessTokenUri: [ ADMINISTRATOR ] }]
